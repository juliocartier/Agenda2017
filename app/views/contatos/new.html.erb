  <div class="container">

        <h1>Novo Contato</h1>

          <%= form_with scope: :contato,url: contatos_path, local: true do |form| %>
        <div class="form-group">
          <div class="field">
              <%= form.label :nome %>
              <%= form.text_field :nome, id: :contato_nome, class: "form-control" %>
            </div>
         </div>

         <div class="form-group">
            <div class="field">
              <%= form.label :endereco %>
              <%= form.text_field :endereco, id: :contato_endereco, class: "form-control" %>
            </div>
          </div>

          <div class="form-group">
            <div class="field">
              <%= form.label :email %>
              <%= form.text_field :email, id: :contato_email, class: "form-control" %>
            </div>
           </div>

         <div class="form-group">
          <div class="field">
              <%= form.label :dataNascimento %>
              <%= form.date_select :dataNascimento, id: :contato_dataNascimento, class: "form-control" %>
            </div>
          </div>

            <div class="actions">
                <%= form.submit  class: "btn btn-outline-primary" %>
            </div>
          <% end %>
      </div>



